set grid
set termoption dashed
set size 0.98
set bmargin 10
set lmargin 18
set rmargin 8
set xtics offset 0,-1,0
set xlabel offset 0,-3,0
set ytics offset -1,-1,0
set ylabel offset -8,0,0
set key font ",30"
set xtics font ",35"
set ytics font ",35" 
set title "Blackscholes Speedup" font ",35"
set ylabel "Speedup" font ",35"
set xlabel "Number of Threads" font ",35"

#set style line 1 lw 1.5 pt 2 ps 5

#set logscale xy # log scaling on both axis

plot "blackscholes_speedup.dat" every ::0::4 using 1:2 title 'Ideal Speedup' with linespoints dt 1 lw 3 ps 3, \
     "blackscholes_speedup.dat" every ::5::9 using 1:2 title '4 Inputs' with linespoints dt 2 lw 3 ps 3, \
     "blackscholes_speedup.dat" every ::10::14 using 1:2 title '16 Inputs' with linespoints dt 3 lw 3 ps 3, \
     "blackscholes_speedup.dat" every ::15::19 using 1:2 title '1000 Inputs' with linespoints dt 4 linecolor 8 lw 3 ps 3, \
     "blackscholes_speedup.dat" every ::20::24 using 1:2 title '4000 Inputs' with linespoints dt 5 linecolor 7 lw 3 ps 3, \
     "blackscholes_speedup.dat" every ::25::29 using 1:2 title '16000 Inputs' with linespoints dt 6 linecolor 6 lw 3 ps 3, \
     "blackscholes_speedup.dat" every ::30::34 using 1:2 title '64000 Inputs' with linespoints dt 7 linecolor 5 lw 3 ps 3 



